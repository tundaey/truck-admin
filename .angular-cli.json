{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "truck-admin"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/css/lib/font-awesome/font-awesome.min.css",
        "assets/css/separate/vendor/bootstrap-select/bootstrap-select.min.css",
        "assets/css/separate/pages/contacts.min.css",
        "assets/css/separate/pages/widgets.min.css",
        "assets/css/separate/pages/others.min.css",
        "assets/css/separate/vendor/datatables-net.min.css",
        "assets/css/lib/datatables-net/datatables.min.css",
        "assets/css/lib/jqueryui/jquery-ui.min.css",
        "assets/css/separate/vendor/lobipanel.min.css",
        "assets/css/lib/lobipanel/lobipanel.min.css",
        "assets/css/separate/pages/user.min.css",
        "assets/css/separate/vendor/select2.min.css",
        "assets/css/lib/bootstrap/bootstrap.min.css",
         "assets/css/main.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
